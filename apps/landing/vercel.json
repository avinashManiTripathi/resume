{
    "buildCommand": "cd ../.. && npm run build:landing",
    "outputDirectory": ".next",
    "installCommand": "cd ../.. && npm install",
    "framework": "nextjs",
    "build": {
        "env": {
            "NODE_VERSION": "20.11.0"
        }
    },
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://api.yourdomain.com/:path*"
        }
    ]
}